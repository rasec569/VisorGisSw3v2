extends ../templates/default

block content
	div(class='row')
		div(class='col-md-6 col-md-offset-3')
			//-  if mensaje.length > 0
			//-   div(class='alert alert-success' role='alert')= mensaje

			div(class='panel panel-default')
				div(class='panel-heading')= 'Registrar Inmueble'
				div(class='panel-body')
					form(action='' method='post' autocomplete='on')
						div(class='form-group')
							label(for="fid")= 'FID'
							input(type='text' name='fid' id='fid' placeholder='FID' class='form-control')
						div(class='form-group')
							label(for="nom")= 'OBJECTID'
							input(type='text' name='objectid' id='objectid' placeholder='OBJECTID' class='form-control')
						div(class='form-group')
							label(for="ape")= 'nombre'
							input(type='text' name='nombre' id='nombre' placeholder='nombre' class='form-control')
						div(class='form-group')
							label(for="nac")= 'Fecha de nacimiento'
							input(type='date' name='nac' id='nac' placeholder='Fecha de nacimiento' class='form-control')
						div(class='form-group')
							label(for="us")= 'Usuario'
							input(type='text' name='us' id='us' placeholder='Usuario' class='form-control')
						div(class='form-group')
							label(for="pas")= 'Password'
							input(type='password' name='pas' id='pas' placeholder='Password' class='form-control')
						div(class='form-group')
							label(for="email")= 'Email'
							input(type='email' name='email' id='email' placeholder='Email' class='form-control')
						div(class='form-group')
							label(for="tel")= 'Telefono'
							input(type='text' name='tel' id='tel' placeholder='Telefono' class='form-control')
						button(type="submit" class='btn btn-default')= 'Registrarme'